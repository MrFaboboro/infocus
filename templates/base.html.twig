		<link rel="stylesheet" href="{{ asset('css/nice-select.css')}}">
		<link rel="stylesheet" href="{{ asset('css/flaticon.css')}}">
		<link rel="stylesheet" href="{{ asset('css/animate.css')}}">
		<link rel="stylesheet" href="{{ asset('css/slicknav.css')}}">
		<style>
			.main-color {
				background-color: rgb(80, 185, 72);
			}
		</style>
		{% block stylesheets %}{% endblock %}
	</head>
	<header>
		<div class="header-area ">
			<div class="header-top black-bg d-none d-md-block">
				<div class="container">
					<div class="row">
						<div class="col-xl-6 col-md-6 col-lg-6">
							<div class="header-contact">
								<a style="color: white; font-size: 16px;" href="#">
									<i class="fa fa-phone" style="color: white; font-size: 16px;"></i>
									+880 256 356 256</a>
								<a style="color: white; font-size: 16px;" href="#">
									<i class="fa fa-envelope" style="color: white; font-size: 16px;"></i>
									support24@infocus.com</a>
							</div>
						</div>
						<div class="col-xl-6 col-md-6 col-lg-6">
							<div class="header-top-menu">
								<nav>
									<ul>
										<li><a href="review.html">Privacy</a></li>
										<li><a href="faq.html">AVG</a></li>
									</ul>
								</nav>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div id="sticky-header" class="main-header-area white-bg">
				<div class="container">
					<div class="row align-items-center">
						<div class="col-xl-2 col-lg-2">
							<div class="logo-img">
								<a href="index.html">
									<img src="img/icon/infocus.png" alt="">
								</a>
							</div>
						</div>
						<div class="col-xl-7 col-lg-7">
							<div class="main-menu d-none d-lg-block">
								<nav>
									<ul id="navigation">
										{% set route = app.request.get('_route') %}
										<li>
											<a class="{{ route starts with 'home' ? 'active' }}" href="{{ path('home') }}">Home</a>
										</li>
										<li>
											<a class="{{ route starts with 'photo' ? 'active' }}" href="{{ path('photo') }}">Photos</a>
										</li>
										<li>
											<a class="{{ route starts with 'projecten' ? 'active' }}" href="{{ path('projecten') }}">Projecten</a>
										</li>
										<li>
											<a class="{{ route starts with 'contact' ? 'active' }}" href="{{ path('contact') }}">Contact</a>
										</li>
									</ul>
								</nav>
							</div>
						</div>
						<div class="col-xl-3 col-lg-3">
							<div class="quote-area">
								<div class="search-bar">
									<a id="search_1" href="javascript:void(0)">
										<i class="fa fa-search"></i>
									</a>
								</div>
								{% if not is_granted('IS_AUTHENTICATED_FULLY') %}
									<div class="get-quote d-none d-lg-block">
										<a class="btn text-white main-color" href="/login">Login</a>
									</div>
								{% else %}
									<div class="get-quote d-none d-lg-block">
										<a class="btn text-white main-color" href="/profile">Profile</a>
									</div>
								{% endif %}

							</div>
						</div>
						<div class="col-12">
							<div class="mobile_menu d-block d-lg-none"></div>
						</div>
					</div>
					<div class="search_input" id="search_input_box">
						<div class="container ">
							<form class="d-flex justify-content-between search-inner">
								<input type="text" class="form-control" id="search_input" placeholder="Zoek...">
								<button type="submit" class="btn"></button>
								<span class="fa fa-close" id="close_search" title="Close Search"></span>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>
